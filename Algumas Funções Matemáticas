import java.util.Scanner;

public class Main {

  	public static void main(String[] args) {
    
    Scanner scan = new Scanner(System.in);
	
    /*

      int variavel
      int variavel_2
    
      raiz = Math.sqrt(variavel);
      potencia = Math.pow(variavel, variavel_2);
      valorAbsoluto = Math.abs(variavel_2);
      maiorValor = Math.max(variavel,variavel_2);

    */
	
  	double pi = 3.14159;
		double a;
		double b;

		a = scan.nextDouble();
		b = scan.nextDouble();
		System.out.printf("Resultado :%.4f%n", pi * Math.pow(a, 2));
		
		System.out.printf("Maior :%.4f%n", Math.max(a, b));
   
    
    }
   }
